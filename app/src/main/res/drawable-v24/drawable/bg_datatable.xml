<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
		<shape>
		    <gradient
                android:startColor="#e6e6e6"
                android:endColor="#d5d5d5"
                android:angle="270" />
            <stroke android:width="1dp" android:color="#b4b3b4" />
            <corners android:radius="5dp" />
            <padding android:left="10dp" android:top="10dp" android:right="10dp" android:bottom="10dp" />
		</shape>
	</item>
	<item>
	    <shape>
	        <stroke android:width="1dp" android:color="#FFF7F7F7"/>
            <padding android:left="1dp" android:top="1dp" android:right="1dp" android:bottom="1dp" />
	    </shape>
	</item>
	<item>
	    <shape>
	        <solid android:color="#FFFFFFFF"/>
	        <stroke android:width="2dp" android:color="#FFBFBFBF"/>
            <padding android:left="5dp" android:top="7dp" android:right="5dp" android:bottom="7dp" />
	    </shape>
	</item>
</layer-list>